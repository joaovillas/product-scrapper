{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.17",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@vitest/coverage-c8": "^0.29.8",
    "dotenv-cli": "^7.1.0",
    "prisma": "^4.11.0",
    "vitest": "^0.29.8",
    "vitest-mock-extended": "^1.1.3"
  },
  "scripts": {
    "dev": "tsx watch ./src/server.ts",
    "start": "tsx ./src/server.ts",
    "prisma": "prisma",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:up": "prisma migrate deploy",
    "prisma:migrate": "prisma migrate dev --name",
    "vercel-build": "yarn prisma:generate",
    "test:unit": "vitest --run --coverage --config ./vitest.config.unit.ts",
    "test:integration": "run-s prepare:integration run:integration",
    "run:integration": "vitest --run --coverage --config ./vitest.config.integration.ts",
    "prepare:integration": "dotenv -e .env.test yarn prisma:up"
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "axios": "^1.3.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "node-html-parser": "^6.1.5",
    "npm-run-all": "^4.1.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.2",
    "tsx": "^3.12.6",
    "typescript": "^5.0.2",
    "zod": "^3.21.4"
  }
